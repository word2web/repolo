// Import and execute the function from your global data file
import getRecipes from "../_data/recipes.ts";

export default async function () {
  console.log("Fetching recipes for /recipes/ page..."); // Debug log
  const recipes = await getRecipes(); // Call the async function
  console.log(`Fetched ${recipes.length} recipes for /recipes/ page.`); // Debug log
  return {
    recipes: recipes // Make the recipes array available as 'data.recipes'
  };
}